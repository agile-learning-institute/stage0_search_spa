{
  "name": "stage0_search_spa",
  "version": "1.0.0",
  "description": "The Search single page application provides a unified search interface across multiple MongoDB collections with Elasticsearch indexing.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "container": "docker build -t ghcr.io/agile-learning-institute/stage0_search_spa:latest ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agile-learning-institute/stage0_search_spa.git"
  },
  "keywords": [
    "vue",
    "search",
    "elasticsearch",
    "mongodb",
    "spa"
  ],
  "author": "Agile Learning Institute",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agile-learning-institute/stage0_search_spa/issues"
  },
  "homepage": "https://github.com/agile-learning-institute/stage0_search_spa#readme",
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.10.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.11"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  }
} 